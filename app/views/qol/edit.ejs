<% include ../partials/whoqol_editheader %>

    <div class="ui container">

            <% include ../partials/menu %>

            <div class="ui piled segment">
                <div class="ui grid">
                    <div class="four wide column">
                        <div class="ui special cards">
                            <div class="card">
                                <div class="blurring dimmable image">
                                    <div class="ui dimmer">
                                        <div class="content">
                                            <div class="center">
                                                <div class="ui inverted button">Profil</div>
                                            </div>
                                        </div>
                                    </div>
                                    <img src="/images/kristy.png">
                                </div>
                                <div class="content">
                                    <div class="ui fluid search selection dropdown">
                                        <input name="beteg" value="<%= teszt.beteg_id %>" form="whoqol" type="hidden">
                                        <i class="dropdown icon"></i>
                                        <div class="default text"><div class="header"><%= teszt.beteg_id %></div></div>
                                        <div class="menu">
                                            <% betegek.forEach(function(beteg){ %>
                                                <div class="item" data-value="<%= beteg.id %>"><%= beteg.azonosito %></div>
                                            <% }); %>
                                        </div>
                                    </div>
                                    <div class="description">
                                        <div class="ui form">
                                            <div class="field">
                                                <textarea name="megjegyzes" form="whoqol" rows="2"><%= teszt.megjegyzes === '' ? 'Megjegyzés hozzáadása' : teszt.megjegyzes %></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="extra content">
                                    <span class="right floated">
                                        <i class="fa fa-clock-o" aria-hidden="true"></i>
                                        <%= moment(teszt.datum).format('LL') %>
                                    </span>
                                    <span>
                                        <div class="ui inline dropdown">
                                            <input name="alkalom" value="<%= teszt.whoqol_alkalom_id %>" form="whoqol" type="hidden">
                                            <i class="fa fa-tag" aria-hidden="true"></i>
                                            <div class="text"><%= teszt.alkalom %></div>
                                            <i class="dropdown icon"></i>
                                            <div class="menu">
                                                <% alkalmak.forEach(function(alkalom){ %>
                                                    <div class="item <% if(teszt.whoqol_alkalom_id == alkalom.id) { %> active selected <% } %>" data-value="<%= alkalom.id %>"><%= alkalom.kod %></div>
                                                <% }); %>
                                            </div>
                                        </div>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
