<script type="text/javascript">
    // Included fastclick.js to make the inputs quicker to respond to taps on mobile

    if ('addEventListener' in document) {
        document.addEventListener('DOMContentLoaded', function() {
            FastClick.attach(document.body);
        }, false);
    }

    // Set form values from JSON
    var tesztadatok = <%- JSON.stringify(teszt) %>;

    var rkerdesek = document.getElementsByClassName("inline-radio");
    for (i = 0; i < rkerdesek.length; i++) {
        var kerdes = rkerdesek[i].getAttribute("id");
        if(tesztadatok[kerdes] === 0) continue;
        var valaszok = document.getElementsByName(kerdes);
        var valasz = tesztadatok[kerdes]-1;
        valaszok[valasz].checked = true;
    }

    var checkboxes = document.querySelectorAll("input[type='checkbox']");
    for(var i = 0; i < checkboxes.length; i++) {
        var kerdes = checkboxes[i].getAttribute("id");
        if(tesztadatok[kerdes] === 1) {checkboxes[i].checked = true;}
    }

</script>

<script>

    // initialize profile image dimmer
    $('.special.cards .image').dimmer({
        on: 'hover'
    });
    // initialize dropdowns
    $('.ui.dropdown').dropdown();

    // toggle egyeb field based on its checkbox
    $(function () {
        $('#egyeb_checkbox').change(function () {
            $('#egyebmezo').toggle(this.checked);
        }).change(); //ensure visible state matches initially
    });

    $(function () {
        $('#beteg_e').change(function () {
            if ($('input[name=beteg_e]:checked', '#whoqol').val() == 2) {
                $('#diagnozismezo').show();
            } else {
                $('#diagnozismezo').hide();
            }
        }).change(); //ensure visible state matches initially
    });

    $('whoqol')
    $('.ui.form')
        .form({
            on: 'blur',
            fields: {
                szulev: {
                    identifier: 'szulev',
                    optional: true,
                    rules: [
                        {
                            type: 'integer[1900..2016]',
                            prompt: 'Helytelen születési dátum (év)'
                        }
                    ]
                },
                szulho: {
                    identifier: 'szulho',
                    optional: true,
                    rules: [
                        {
                            type: 'integer[1..12]',
                            prompt: 'Helytelen születési dátum (hónap)'
                        }
                    ]
                },
                szulnap: {
                    identifier: 'szulnap',
                    optional: true,
                    rules: [
                        {
                            type: 'integer[1..31]',
                            prompt: 'Helytelen születési dátum (nap)'
                        }
                    ]
                },
            }
        });

</script>

