<script type="text/javascript">
    var tesztadatok = <%- JSON.stringify(rows[0]) %>;

    var rkerdesek = document.getElementsByClassName("inline-radio");
    for (i = 0; i < rkerdesek.length; i++) {
        var kerdes = rkerdesek[i].getAttribute("id");
        if(tesztadatok[kerdes] === 0) continue;
        var valaszok = document.getElementsByName(kerdes);
        var valasz = tesztadatok[kerdes]-1;
        valaszok[valasz].checked = true;
    }

    var checkboxes = document.querySelectorAll("input[type='checkbox']");
    for(var i = 0; i < checkboxes.length; i++) {
        var kerdes = checkboxes[i].getAttribute("id");
        if(tesztadatok[kerdes] === 1) {checkboxes[i].checked = true;}
    }
</script>

